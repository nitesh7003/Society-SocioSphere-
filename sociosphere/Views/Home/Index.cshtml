                    @model sociosphere.Models.ContactFormModel

                    <div id="contact-section" class="container-fluid contact bg-light py-5">
                        <div class="container py-5">
                            <div class="text-center mx-auto pb-5 wow fadeInUp" data-wow-delay="0.2s" style="max-width: 800px;">
                                <h4 class="text-primary">Contact Us</h4>
                                <h1 class="display-4 mb-4">If you have any comments please apply now</h1>
                            </div>
                            <div class="row g-5">
                                <div class="col-xl-6 wow fadeInLeft" data-wow-delay="0.2s">
                                    <div class="contact-img d-flex justify-content-center">
                                        <div class="contact-img-inner">
                                            <img src="/Content/Images/illustration-teamwork-harmony-diversity-with-circle-colorful-leaves_1300528-8820-removebg-preview.png" class="img-fluid w-100" alt="Image">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-xl-6 wow fadeInRight" data-wow-delay="0.4s">
                                    <div>
                                        <h4 class="text-primary">Send Your Message</h4>
                                        <p class="mb-4">The contact form is currently inactive. Get a functional and working contact form with Ajax & PHP in a few minutes. Just copy and paste the files, add a little code and you're done.</p>

                                        <!-- Display success message if available -->
                                        @if (TempData["SuccessMessage"] != null)
                                        {
                                            <div class="alert alert-success">
                                                @TempData["SuccessMessage"]
                                            </div>
                                        }

                                        <!-- Contact Form -->
                                        <form asp-action="Submit" method="post">
                                            <div class="row g-3">
                                                <div class="col-lg-12 col-xl-6">
                                                    <div class="form-floating">
                                                        <input type="text" class="form-control border-0" id="name" name="Name" placeholder="Your Name" value="@Model?.Name">
                                                        <label for="name">Your Name</label>
                                                        <span asp-validation-for="Name" class="text-danger"></span>
                                                    </div>
                                                </div>
                                                <div class="col-lg-12 col-xl-6">
                                                    <div class="form-floating">
                                                        <input type="email" class="form-control border-0" id="email" name="Email" placeholder="Your Email" value="@Model?.Email">
                                                        <label for="email">Your Email</label>
                                                        <span asp-validation-for="Email" class="text-danger"></span>
                                                    </div>
                                                </div>
                                                <div class="col-lg-12 col-xl-6">
                                                    <div class="form-floating">
                                                        <input type="phone" class="form-control border-0" id="phone" name="Phone" placeholder="Phone" value="@Model?.Phone">
                                                        <label for="phone">Your Phone</label>
                                                        <span asp-validation-for="Phone" class="text-danger"></span>
                                                    </div>
                                                </div>
                                                <div class="col-lg-12 col-xl-6">
                                                    <div class="form-floating">
                                                        <input type="text" class="form-control border-0" id="project" name="Project" placeholder="Project" value="@Model?.Project">
                                                        <label for="project">Your Project</label>
                                                    </div>
                                                </div>
                                                <div class="col-12">
                                                    <div class="form-floating">
                                                        <input type="text" class="form-control border-0" id="subject" name="Subject" placeholder="Subject" value="@Model?.Subject">
